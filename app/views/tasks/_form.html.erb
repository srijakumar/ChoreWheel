<%= form_for([@chore, @task]) do |f|%>
   <%= f.text_field :content, placeholder: "New Task" %>
   <%= f.fields_for :lists do |lists_fields| %>
    <%= lists_fields.text_field :title, placeholder: "Task - Note Title" %>
    <%= lists_fields.text_field :content, placeholder: "Task - Note Content" %>
  <% end %>
  <%= f.submit %>
<% end %>
